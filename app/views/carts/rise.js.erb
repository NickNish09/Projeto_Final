$(document).ready(function(){
	console.log("pudim");
	var id = <%= params[:product] %>;
	var quantidade = parseInt($('#fieldQt'+id).val());
	var preco = <%= params[:prc] %>;
	var tot = parseFloat($('#total').val());
	quantidade += 1;
	tot += preco;
	tot = tot.toFixed(2);
	$('#fieldQt'+id).val(quantidade);
	$('#priceQty'+id).val((quantidade*preco).toFixed(2));
	$('#total').val(tot);
		

});
